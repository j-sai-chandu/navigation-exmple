{
    "name": "costar/flash",
    "description": "Easy flash notifications",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^6.1|^9.5.10"
    },
    "autoload": {
        "psr-0": {
            "Costar\\Flash": "src/"
        },
        "files": [
            "src/Costar/Flash/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Costar\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "Costar\\Flash\\Flash"
            }
        }
    }
}
