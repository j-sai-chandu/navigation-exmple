{
    "name": "costar/laravel-menu",
    "description": "A quick way to create menus in Laravel",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Costar",
            "email": "costar@local.dev"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0",
        "illuminate/view": ">=5.0"
    },
    "autoload": {
        "psr-0": {
            "Costar\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Costar\\Menu\\ServiceProvider"
            ],
            "aliases": {
                "Menu": "Costar\\Menu\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
